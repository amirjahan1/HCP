{
  "manifest_version": 3,
  "name": "Storage Manager",
  "version": "1.0",
  "description": "Manage and export/import local storage, session storage, and cookies.",
  "permissions": [
    "storage",
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}